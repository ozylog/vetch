{
  "name": "vetch",
  "version": "0.0.0",
  "description": "Simplified fetch that run on browser and node",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.9.0 <9.0",
    "yarn": ">=1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ozylog/vetch.git"
  },
  "keywords": [
    "fetch",
    "graphql"
  ],
  "contributors": [
    "Cahya Pribadi <cahya@monelog.com>"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/ozylog/vetch/issues"
  },
  "homepage": "https://github.com/ozylog/vetch.git#readme",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --coverage"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.0.0",
    "jest": "^23.1.0",
    "nock": "^9.3.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  }
}
