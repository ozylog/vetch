{
  "name": "vetch",
  "version": "0.3.1",
  "description": "Simple isomorphic fetch",
  "main": "./dist/index.js",
  "engines": {
    "node": ">=8.9.0 <11.0",
    "yarn": ">=1.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ozylog/vetch.git"
  },
  "keywords": [
    "fetch"
  ],
  "contributors": [
    "Cahya Pribadi <ozylog@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ozylog/vetch/issues"
  },
  "homepage": "https://github.com/ozylog/vetch.git#readme",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest --coverage",
    "coverage": "cat ./coverage/lcov.info | yarn coveralls"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "isomorphic-fetch": "^2.2.1"
  },
  "devDependencies": {
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.15",
    "@types/nock": "^10.0.3",
    "coveralls": "^3.0.4",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-console": 1,
      "no-useless-escape": 0,
      "@typescript-eslint/indent": [
        "error",
        2
      ],
      "@typescript-eslint/explicit-function-return-type": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "args": "none"
        }
      ]
    }
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json",
      "node"
    ]
  }
}
